datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id        String   @id @default(cuid())
  name      String
  price     Float
  seller    Seller   @relation(fields: [sellerId], references: [id])
  sellerId  String
}

model Seller {
  id       String    @id @default(cuid())
  name     String
  products Product[]
}